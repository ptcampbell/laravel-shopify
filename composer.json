{
    "name": "theme-anorak/laravel-shopify",
    "description": "Api Wrapper for using Shopify in a Laravel application",
    "type": "library",
    "license": "MIT",
    "require": {
        "dpc/guzzle-client": "^0.3.1",
        "dpc/hash-verifier": "^0.2.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "ThemeAnorak\\LaravelShopify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ThemeAnorak\\LaravelShopify\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ThemeAnorak\\LaravelShopify\\Providers\\LaravelShopifyServiceProvider",
                "ThemeAnorak\\LaravelShopify\\Providers\\ShopifyAuthServiceProvider"
            ]
        }
    },

    "prefer-stable": true,
    "minimum-stability": "dev"
}
